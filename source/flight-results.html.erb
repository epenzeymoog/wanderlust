<div class="full-to-two-thirds right">
		<div class="user-selection">

			<div class="half-to-third--secondary">
				<label>From</label>
	      		<input type="text" name="from" placeholder="Chicago ORD">
	      <label>Departing</label>
	       	<input type="date" name="departing" required placeholder="11/19/2016">
			</div>

			<div class="half-to-third--secondary">
       	<label class="block">To</label>
      		<input type="text" name="to" placeholder="Amsterdam ORS">
				<label>Returning</label>
	       	<input type="date" name="returning" required placeholder="11/29/2016">
	    </div>

			<div class="quarter-to-hide">
				<label>Adults</label>
				<select name="Adults">
					<option value="more filters selected">1 Adults</option>
				  <option value="volvo">2 Adults</option>
				  <option value="saab">3 Adults</option>
				  <option value="fiat">4 Adults</option>
				</select>
			</div>
				
			<div class="quarter-to-hide">
				<label>Kids</label>
				<select name="Kids">
					<option value="more filters selected">0 Kids</option>
				  <option value="volvo">2 Kids</option>
				  <option value="saab">3 Kids</option>
				  <option value="fiat">4 Kids</option>
				</select>
			</div>

			<div class="hide-to-twelfth">
				<label>Adults</label>
				<select name="Adults">
					<option value="more filters selected">1 Adult</option>
				  <option value="volvo">2 Adults</option>
				  <option value="saab">3 Adults</option>
				  <option value="fiat">4 Adults</option>
				</select>

				<label>Kids</label>
				<select name="Kids">
					<option value="more filters selected">0 Kids</option>
				  <option value="volvo">2 Kids</option>
				  <option value="saab">3 Kids</option>
				  <option value="fiat">4 Kids</option>
				</select>
			</div>
			

			<div class="half-to-quarter">
			
				<input type="button" value="Update" class="button button--secondary" onclick="location.href='/flight-results.html';">
			</div>
		</div>


	<section class="card--wide card--alternative">
