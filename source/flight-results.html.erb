---
title: Wanderlust
layout: layout
---

<%= partial "partials/nav" %>

<div class="container margin-top">

	<div class="one-third left hide-until-large">
		<section class="form-column">
			<input type="button" value="Create price alert" class="button button--tertiary left" onclick="location.href='/flight-results.html';">

			<div class="full block">
				<select class="sort-dropdown">
					<option value="sort-results">Sort results</option>
					<option value="price-low-to-high">Price: Lowest to highest</option>
					<option value="price-high-to-low">Price: Highest to lowest</option>
					<option value="duration-short-to-long">Duration: shortest to longest</option>
					<option value="duration-long-to-short">Duration: longest to shortest</option>
					<option value="takeoff-early-to-late">Takeoff: earliest to latest</option>
					<option value="takeoff-late-to-early">Takeoff: latest to earliest</option>
				</select>
			</div>
				
			<ul class="form-column__title"><p class="hide-ms">Price</p>
				<div class="form-column__range-values hide-ms">
					<li><p class="form-column__label left">$955</p></li>
					<li><p class="form-column__label right">$1944</p></li>
				</div>
				<input class="hide-ms" type="range" value=0><input class="hide-ms" type="range" value=100>
			</ul>

			<ul class="form-column__title"><p>View flights by:</p>
				<li><label class="form-column__label"><input type="checkbox">roundtrip</label></li>
				<li><label class="form-column__label"><input type="checkbox">flight-by-flight</label></li>
			</ul>

			<ul class="form-column__title"><p>Stops</p>
				<li><label class="form-column__label"><input type="checkbox">nonstop</label></li>
				<li><label class="form-column__label"><input type="checkbox">1 stop</label></li>
				<li><label class="form-column__label"><input type="checkbox">2+ stops</label></li>
			</ul>

			<ul class="form-column__title hide-ms"><p>Times</p>
				<li><p class="form-column__subtitle hide-ms">Take-off: Chicago</p></li>
				<li><p class="form-column__label">Friday 5:30am - 10:00pm</p></li>
				<li><input type="range" value=0><input type="range" value=100></li>
			
				<li><p class="form-column__subtitle">Take-off: Amsterdam</p></li>
				<li><p class="form-column__label">Friday 5:30am - 10:00pm</p></li>
				<li><input type="range" value=0><input type="range" value=100></li>
			</ul>

			<ul class="form-column__title"><p>Airports</p>
				<ul class="form-column__subtitle"><p>Chicago</p>
					<li><label class="form-column__label"><input type="checkbox">ORD: O'Hare International</label></li>
					<li><label class="form-column__label"><input type="checkbox">MDW: Midway</label></li>
				</ul>
				<ul class="form-column__subtitle"><p>Amsterdam</p>
					<li><label class="form-column__label"><input type="checkbox">AMS: Schiphol Airport</label></li>
				</ul>
			</ul>

			<ul class="form-column__title"><p>Airlines</p>
				<li>
					<% ["Aer Lingus", "Aeromexico", "Air Canada", "Air Europe", "Air France", "Alitalia", "American Airlines", "ANA", "Australian Airlines", "British Airways", "Cathay Pacific", "Copa Airlines", "Delta", "Emirates", "Etihad Airways", "EVA Air", "Finnair", "Iberia", "Jet Airways", "KLM", "LOT Polish Airlines", "SWISS", "United"].each do |airline| %>
						<label class="form-column__label"><input type="checkbox"><%= airline %></label>
					<% end %>
				</li>
			</ul>
		</section>

	<%= partial "partials/calendar" %>

