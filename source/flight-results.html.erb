	      <label>Departing</label>
	       	<input type="text" id="datepicker0" placeholder="11/19/2016">
			</div>

			<div class="half-to-third">
       	<label class="block">To</label>
      		<input type="text" name="to" placeholder="Amsterdam ORS">
				<label>Returning</label>
	        <input type="text" id="datepicker01" placeholder="11/29/2016">
	    </div>

			<div class="quarter-to-hide">
				<label>Adults</label>
